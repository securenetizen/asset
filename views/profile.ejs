<!DOCTYPE html>
<html>
  <head>
    <title>Profile</title>
  </head>
  <body>
    <h1><%= user.username %>'s Profile</h1>
    <p>Role: <%= user.role %></p>
    <h2>Allocated Items</h2>
    <ul>
      <% allocations.forEach(function(a){ %>
        <li><%= a.name %></li>
      <% }) %>
    </ul>
    <a href="/dashboard">Back</a>
  </body>
</html>

